{
  "name": "enumerable",
  "version": "0.0.1",
  "description": "Rails's Enumerable methods strictly typed",
  "main": "dist/index.js",
  "author": "hwadii",
  "license": "MIT",
  "devDependencies": {
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "prepare": "yarn build"
  },
  "ava": {
